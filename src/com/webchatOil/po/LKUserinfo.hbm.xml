<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.webchatOil.po">
	<class name="LKUserinfo" table="LKUserinfo">
		<id name="E_id" type="java.lang.Integer>
			<generator class="assigned"></generator>
		</id>
		<property name="D_id" type="java.lang.Integer" not-null="true"></property>
		<property name="E_name" type="java.lang.String" not-null="true"></property>
		<property name="loginname" type="java.lang.String"></property>
		<property name="pwd" type="java.sql.String"></property>
		<property name="sex" type="java.sql.String"></property>
		<property name="birthday" type="java.sql.Date"></property>
	    <property name="phoneH" type="java.sql.String"></property>
		<property name="phoneM" type="java.sql.String"></property>
		<property name="Address" type="java.sql.String"></property>
		<property name="popedom" type="java.sql.String"></property>
	</class>
</hibernate-mapping>  

